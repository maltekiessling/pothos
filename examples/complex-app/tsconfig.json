{
  "$schema": "https://json.schemastore.org/tsconfig.json",
  "compilerOptions": {
    "strict": true,
    "skipLibCheck": false,
    "target": "es2019",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "module": "ESNext"
  },
  "include": [
    "prisma/*.ts",
    "src/**/*",
    "scripts/**/*"
  ]
}
